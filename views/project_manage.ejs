    <div id="page-wrapper">

        <div class="container-fluid">

            <!-- Page Heading -->
            <div class="row">
                <div class="col-lg-12">
                    <h1 class="page-header">
                        프로젝트 관리
                    </h1>
                    <ol class="breadcrumb">
                        <li class="active">
                            <i class="fa fa-dashboard"></i>그룹 : <%= project_group_list[0].groupname %>
                        </li>
                    </ol>
                </div>
            </div>
            <!-- /.row -->

            <div class="row">
                <div class="col-lg-12">
                </div>
            </div>
            <!-- /.row -->

            <!-- /.row -->

            <div class="row">
                <div class="col-lg-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-bar-chart-o fa-fw"></i> 프로젝트 목록</h3>
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">
                                <table class="table">
                                    <tr>
                                        <th>project name</th>
                                        <th>create date</th>
                                        <th>git url</th>
                                    </tr>
                                    <% project_group_list.forEach(function(item, index){ %>
                                    <tr>
                                        <td><a href="project/<%= item.id%>">
                                                <%= item.project_name %>
                                            </a>
                                        </td>
                                        <td>
                                            <%= item.create_date %>
                                        </td>
                                        <td>
                                            <%= item.git_url %>
                                        </td>
                                    </tr>
                                    <% });%>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.row -->

            <div class="row">
                <div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-long-arrow-right fa-fw"></i>프로젝트 추가</h3>
                        </div>
                        <div class="panel-body">
                            <div class="text-left">
                                <form action="" method="post" enctype="multipart/form-data">
                                    <div class="table-responsive">
                                        <table class="table">
                                            <tr>
                                                <td>git url </td><td><input type="text" name="git_url" required></td>
                                            </tr>
                                            <tr>
                                                <td>project name </td><td><input type="text" name="project_name" required></td>
                                            </tr>
                                            <tr>
                                                <td>makefile</td><td><input type="file" name="make_file" required></td>
                                            </tr>
                                        </table>
                                        <input class="btn btn-default" type="submit" value="생성">
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-clock-o fa-fw"></i></h3>
                        </div>
                        <div class="panel-body">
                            <div class="list-group">
                            </div>
                            <div class="text-right">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="panel panel-default">
                        <div class="panel-heading">
                            <h3 class="panel-title"><i class="fa fa-money fa-fw"></i></h3>
                        </div>
                        <div class="panel-body">
                            <div class="table-responsive">

                            </div>
                            <div class="text-right">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.row -->

        </div>
        <!-- /.container-fluid -->

    </div>
    <!-- /#page-wrapper -->